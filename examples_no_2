# Word splitter and emojis
'''
message = input("> ")
word_list = message.split(" ")
print(message)
print(word_list)
emoji = {
    ":)": "(Happy Smilie Face)",
    ":(": "(Sad Smilie Face)",
    ":*": "(Kiss Smilie face)"
}
output = ""
for word in word_list:
    output += emoji.get(word, word) + " "
print(output)
'''


# Emoji converter function (INCOMPLETE)

'''
def emoji_converter(string):
    word_list = string.split(" ")
    print(f"Word list: '{word_list}'")
    emotes = {
        ":)": "(Happy Smilie Face)",
        ":(": "(Sad Smilie Face)",
        ":*": "(Kiss Smilie face)"
    }
    output = ""
    for word in word_list:
        output = output + emotes.get(word, word) + " "
        return output


message = input("> ")
print(f" User message is '{message}'")
result = emoji_converter(message)
print(f"Result is '{result}'")
'''

